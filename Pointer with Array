#include <stdio.h>
#include <stdlib.h>

int main()
{
 int a[]={5,8,2,7,5,4,1,6,9,5};
 int *p,c,*q;

 printf("First index = %d\n",&a[0]);

 printf("Last index = %d\n\n\n",&a[c-1]);


 c=sizeof(a)/sizeof(a[0]);
 for(int i=0;i<c;i++)
 {
     p[i]=&a[i];
     printf("Value : %d\n",a[i]);
     printf("Address : %d\n\n",p[i]);


 }



}
